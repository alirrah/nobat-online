{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^(vite|@vitejs)(/(.*))?$",
    "^(react|react-dom)(/(.*))?$",
    "^next(/(.*))?$",
    "^@prisma(/(.*))?$",
    "^clsx(/(.*))?$",
    "<THIRD_PARTY_MODULES>",
    "@/assets",
    "@/components",
    "@/context",
    "@/dto",
    "@/enums",
    "@/hooks",
    "@/icons",
    "@/lib",
    "@/logo",
    "@/mock",
    "@/models",
    "@/providers",
    "@/types",
    "@/utils",
    "@/styles",
    "^(\\.|\\.\\.)/(.(?!.(css|scss)))*$",
    "\\.(css|scss)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderGroupNamespaceSpecifiers": true
}
